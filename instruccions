# Subdominios
sublist3r -d uber.com -o data/subdomains.txt
# Wordlist especÃ­fica para Uber
echo -e "rides\neats\nfreight\ndriver\npassenger\napi-prod\ndata-prod\nbackup-2025\nlogs-2024" > data/uber_wordlist.txt
# Combinar wordlists
wget https://raw.githubusercontent.com/danielmiessler/SecLists/master/Discovery/Web-Content/s3-buckets.txt -O data/s3-buckets.txt
cat /usr/share/wordlists/SecLists/Discovery/Web-Content/raft-large-words-lowercase.txt \
    data/s3-buckets.txt \
    data/uber_wordlist.txt \
    | sort -u | grep -E '^[a-z0-9][a-z0-9.-]*$' | grep -v '\.\.' > data/s3_wordlist.txt
# Wordlist para archivos sensibles
cat /usr/share/wordlists/SecLists/Discovery/Web-Content/raft-large-files-lowercase.txt \
    /usr/share/wordlists/SecLists/Discovery/Web-Content/Common-DB-Backups.txt \
    | sort -u > data/grep_words.txt


